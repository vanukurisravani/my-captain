#include<stdio.h>
void main()
{
int marks;
printf("Enter your marks")
scanf("%d",&marks)
if(marks<0 || marks>100)
{
  printf("Wrong Entry")
}
else if(marks>84)
{
  printf("Grade A")
}
else if(marks>69)
{
  printf("Grade B")
}
else if(marks>54)
{
  printf("Grade C")
}
else if(marks>39)
{
  printf("Grade D")
}
else
{
  printf("Grade F")
}
}

