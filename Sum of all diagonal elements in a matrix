#include<studio.h>
void main()
{
  int mat[10][10],i,j,row,col,sum=0
  printf("Enter the number of rows and columns for matrix\n")
  scanf("%d%d",&row,&col);
  printf("element-mat[%d][%d]\n",i,j);
  for(i=0;i<row;i++)
  {
    for(j=0;j<col;j++)
    {
      scanf("%d",&mat[i][j]);
     }
  }
printf("the matrix\n")
for(i=0;i<row;i++)
{
  for(j=o;j<col;j++)
   {
     printf("%d\t",mat[i][j]);
    }
     printf("\n")
 }
for(i=0;i<row;i++)
{
   if(i==j)
   {
      sum=sum+mat[i][j]
    }
  }
}
printf("the sum of diagonal elements of a square matrix=%d\n",sum)
